{
  "name": "nexmo-cli",
  "version": "0.4.9-alpha-1",
  "description": "Nexmo Command Line Interface",
  "main": "lib/request.js",
  "scripts": {
    "prebuild": "eslint src",
    "build": "babel -d lib src/ -s inline",
    "lint": "eslint .",
    "lint:fix": "eslint --fix .",
    "pretest": "eslint tests",
    "test": "mocha --require @babel/register tests/*",
    "watch:test": "watch 'npm test' tests src",
    "prepublish": "npm run build",
    "debug": "node --nolazy --inspect-brk=9229 lib/bin.js"
  },
  "repository": {
    "type": "git",
    "url": "git://github.com/nexmo/nexmo-cli.git"
  },
  "keywords": [
    "nexmo",
    "sms",
    "voice",
    "cli"
  ],
  "bin": {
    "nexmo": "lib/bin.js"
  },
  "author": "nexmo",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/nexmo/nexmo-cli/issues"
  },
  "contributors": [
    "cbetta",
    "leggetter",
    "alexlakatos"
  ],
  "homepage": "https://github.com/nexmo/nexmo-cli",
  "devDependencies": {
    "@babel/cli": "^7.1.2",
    "@babel/core": "^7.1.2",
    "@babel/preset-env": "^7.1.0",
    "@babel/register": "^7.0.0",
    "chai": "^4.2.0",
    "eslint": "^5.8.0",
    "mocha": "^5.2.0",
    "sinon": "^7.1.1",
    "sinon-chai": "^3.2.0",
    "sinon-test": "^2.4.0",
    "watch": "^1.0.2"
  },
  "dependencies": {
    "colors": "^1.3.2",
    "commander": "^2.19.0",
    "ini": "^1.3.4",
    "nexmo": "2.5.3-beta-3"
  },
  "engines": {
    "node": ">=6.0.0"
  }
}
